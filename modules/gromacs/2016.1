#%Module1.0
proc ModulesHelp { } {
global dotversion
 
puts stderr "\tGROMACS 2016.1"
}
 
module-whatis "GROMACS 2016.1"
prereq cuda/8.0
conflict gromacs

setenv GMXPREFIX /store/packages/gromacs/gromacs-2016.1
setenv GMXBIN /store/packages/gromacs/gromacs-2016.1/bin
setenv GMXLDLIB /store/packages/gromacs/gromacs-2016.1/lib
setenv GMXMAN /store/packages/gromacs/gromacs-2016.1/share/man
setenv GMXDATA /store/packages/gromacs/gromacs-2016.1/share/gromacs
setenv GROMACS_DIR /store/packages/gromacs/gromacs-2016.1

prepend-path LD_LIBRARY_PATH /store/packages/gromacs/gromacs-2016.1/lib
prepend-path PKG_CONFIG_PATH /store/packages/gromacs/gromacs-2016.1/lib/pkgconfig
prepend-path PATH /store/packages/gromacs/gromacs-2016.1/bin
prepend-path MANPATH /store/packages/gromacs/gromacs-2016.1/share/man
