#%Module1.0
proc ModulesHelp { } {
global dotversion

puts stderr "\tAMBER 18"
}

module-whatis "AMBER 18"
conflict amber

setenv AMBER_PREFIX /packages/amber/18
setenv AMBERHOME /packages/amber/18
prepend-path PATH /packages/amber/18/bin
prepend-path PYTHONPATH /packages/amber/18/lib/python/site-packages
prepend-path LD_LIBRARY_PATH /packages/amber/18/lib

